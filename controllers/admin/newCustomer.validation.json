{
  "cnpj": {
    "format": {
      "pattern": "[0-9]{2}[.]?[0-9]{3}[.]?[0-9]{3}[/]?[0-9]{4}[-]?[0-9]{2}",
      "message": "^CNPJ is not in a valid format"
    },
    "length": {
      "minimum": 14,
      "maximum": 18
    },
    "cnpj": "^CNPJ is not a valid value"
  },
  "cpf": {
    "format": {
      "pattern": "[0-9]{3}[.]?[0-9]{3}[.]?[0-9]{3}[-]?[0-9]{2}",
      "message": "^CPF is not in a valid format"
    },
    "length": {
      "minimum": 9,
      "maximum": 14
    },
    "cpf": "^CPF is not valid value"
  },
  "name": {
    "presence": {
      "allowEmpty": false
    }
  },
  "email": {
    "email": true,
    "presence": {
      "allowEmpty": false
    }
  },
  "credential": {
    "length": {
      "minimum": 6
    },
    "presence": {
      "allowEmpty": false
    }
  }
}